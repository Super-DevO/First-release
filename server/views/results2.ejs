<%- include partials/header.ejs %>

<%- include partials/ylistHeader.ejs %>

<div class="container">
    <div class="jumbotron">
        <h1><%= title %>: <%= survey.Name %></h1>
    </div>

    <% for(let count = 0; count < survey.quearray.length; count++) { let i = 1; %>
      <div class="my-3 p-3 bg-white rounded shadow-sm">
        <h6 class="border-bottom border-gray pb-2 mb-0">@anon: <%= count %></h6>
        <div class="media text-muted pt-3">
          <svg class="bd-placeholder-img mr-2 rounded" width="32" height="32" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMidYMid slice" focusable="false" role="img" aria-label="Placeholder: 32x32"><title>Placeholder</title><rect width="100%" height="100%" fill="#007bff"/><text x="50%" y="50%" fill="#007bff" dy=".3em">32x32</text></svg>
          <p class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray">
            <strong class="d-block text-gray-dark">Responses</strong>
            <% for(let j = 0; j < RespList[j].length; j++) { %>
              <%= survey.quearray[j].question %>: <%= RespList[i][j].resp %><br>
              
            <% } %>
          </p>
        </div>
      </div>
    <% i++; } %>
</div>
      
<%- include partials/bottom_nav.ejs %>  

<%- include partials/footer.ejs %>
